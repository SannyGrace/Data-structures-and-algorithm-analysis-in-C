#include <stdio.h>
void print_out(unsigned int n);
void print_digit(unsigned int n);
int main() {
    // Write C code here
    unsigned int n = 1237832;
    print_out(n);
    
    return 0;
}

void print_out(unsigned int n){
     if (n<10)
       print_digit(n);
     else
     {
         print_out(n/10);
         print_digit(n%10);
     }
}

void print_digit(unsigned int n){
    
    printf("%d",n);
}
